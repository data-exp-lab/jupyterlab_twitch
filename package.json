{
  "name": "@data-exp-lab/jupyterlab-twitch",
  "version": "0.1.0",
  "description": "A JupyterLab extension.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/matthewturk/jupyterlab_twitch",
  "bugs": {
    "url": "https://github.com/matthewturk/jupyterlab_twitch/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Matthew Turk",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/matthewturk/jupyterlab_twitch.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib tsconfig.tsbuildinfo",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "prepare": "jlpm run clean && jlpm run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^3.0.0",
    "@jupyterlab/application": "^2.0.0",
    "@jupyterlab/apputils": "^2.1.1",
    "@lumino/coreutils": "^1.5.0",
    "@lumino/widgets": "^1.13.0",
    "@types/node": "^14.0.13"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "prettier": "1.16.4",
    "rimraf": "^2.6.1",
    "typescript": "~3.7.0"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "jupyterlab": {
    "extension": "lib/plugin"
  }
}
